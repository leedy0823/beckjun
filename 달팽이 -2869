#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <algorithm>

using namespace std;

int main()
{
	int A, B, V;
	int date = 0;
	int temp = 0;
	cin >> A >> B >> V;

	temp = V - A;
	temp = temp / (A - B);
	date = temp - 2;
	V = V - date * (A - B);

	while (1)
	{
		V = V - A;
		date++;
		if (V <= 0)
			break;
		V = V + B;
	}
	cout << date;

	return 0;
}
